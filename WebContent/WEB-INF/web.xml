<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Examples</display-name>
  <servlet>
    <servlet-name>1</servlet-name>
    <servlet-class>com.olabsys.project.LoginAction</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>2</servlet-name>
    <servlet-class>com.olabsys.project.InsertRecord</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>1</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>2</servlet-name>
    <url-pattern>/InsertRecord</url-pattern>
  </servlet-mapping>
</web-app>